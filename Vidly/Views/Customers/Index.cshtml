@model Vidly.ViewModels.CustomersViewModel
@{
    ViewBag.Title = "Customers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customers</h2>

<ul>
    @foreach (var customer in Model.Customers)
    {
        <a href="http://customer/" target="_blank">@customer.Name</a>
        <li>@customer.Name</li>
    }
</ul>
